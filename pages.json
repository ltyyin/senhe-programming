{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationStyle":"custom",
				"navigationBarTitleText": "森和编程",
				"enablePullDownRefresh": false
			}
		}, 
		{
			"path": "pages/category/category",
			"style": {
				"navigationBarTitleText": "分类",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/read/read",
			"style": {
				"navigationBarTitleText": "阅读",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/question/question",
			"style": {
				"navigationBarTitleText": "问答",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
				// #ifdef APP-PLUS
				,"app-plus": {
					"titleNView": {
						"buttons": [
							{
								"fontSrc": "/static/iconfont/iconfont.ttf",
								"text": "\ue891"
							}
						]
					}
				}
				// #endif
			}
		},
		{
			"path": "pages/public/web-view",
			"style": {
				"navigationBarTitleText": "web-view",
				"enablePullDownRefresh": false,
				"app-plus": {
					"bounce":"none"
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"backgroundColorTop":"#FFF"
	},
	"tabBar":{
		"color":"#BFBFBF",
		"backgroundColor":"#ffffff",
		"selectedColor":"#1eddb2",
		"borderStyle":"white",
		// #ifdef APP-PLUS || H5
		"height":"52px",
		// #endif
		"list":[
			{
				"text":"首页",
				"pagePath":"pages/index/index",
				"iconPath":"static/tabIcon/home.png",
				"selectedIconPath":"static/tabIcon/home_active.png"
			},
			{
				"text":"分类",
				"pagePath":"pages/category/category",
				"iconPath":"static/tabIcon/category.png",
				"selectedIconPath":"static/tabIcon/category_active.png"
			},
			{
				"text":"阅读",
				"pagePath":"pages/read/read",
				"iconPath":"static/tabIcon/read.png",
				"selectedIconPath":"static/tabIcon/read_active.png"
			},
			{
				"text":"问答",
				"pagePath":"pages/question/question",
				"iconPath":"static/tabIcon/question.png",
				"selectedIconPath":"static/tabIcon/question_active.png"
			},
			{
				"text":"我的",
				"pagePath":"pages/my/my",
				"iconPath":"static/tabIcon/my.png",
				"selectedIconPath":"static/tabIcon/my_active.png"
			}
		]
	}
